# Firebase Configuration
# Get these from your Firebase project settings
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project_name.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id_here
VITE_FIREBASE_STORAGE_BUCKET=your_project_id_here.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id_here
VITE_FIREBASE_APP_ID=your_app_id_here

# Base44 Configuration
# Get these from your Base44 dashboard
VITE_BASE44_API_KEY=your_base44_api_key_here
VITE_BASE44_APP_ID=your_base44_app_id_here

# Development/Production
NODE_ENV=development